{
"login": {
    "emailInput": "#email",
    "passwordInput": "#password",
    "submitBtn": "button",
    "pageTitle": ".title-style",
    "errorAlert": ".alert"
},

"header": {
    "loginBtn": "a[href='/login']",
    "logoutBtn": ".ml-auto > :nth-child(3) > .nav-link",
